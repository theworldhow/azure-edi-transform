  <!-- MOA - Outpatient Adjudication -->
  <xs:complexType name="MOA_Type">
    <xs:sequence>
      <xs:element name="MOA01" type="xs:string" minOccurs="0"/>
      <xs:element name="MOA02" type="xs:string" minOccurs="0"/>
      <xs:element name="MOA03" type="xs:string" minOccurs="0"/>
    </xs:sequence>
  </xs:complexType>

  <!-- SVC - Service Information -->
  <xs:complexType name="SVC_Type">
    <xs:sequence>
      <xs:element name="SVC01" type="x12:SVC01_Type"/>
      <xs:element name="SVC02" type="xs:string"/>
