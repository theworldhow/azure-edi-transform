      <xs:element name="SVC0103" type="xs:string" minOccurs="0"/>
      <xs:element name="SVC0104" type="xs:string" minOccurs="0"/>
    </xs:sequence>
  </xs:complexType>

  <!-- AMT - Monetary Amount -->
  <xs:complexType name="AMT_Type">
