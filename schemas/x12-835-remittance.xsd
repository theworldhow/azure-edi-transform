      <xs:element name="AMT01" type="xs:string"/>
      <xs:element name="AMT02" type="xs:string"/>
    </xs:sequence>
  </xs:complexType>

  <!-- QTY - Quantity -->
  <xs:complexType name="QTY_Type">
    <xs:sequence>
      <xs:element name="QTY01" type="xs:string"/>
      <xs:element name="QTY02" type="xs:string"/>
    </xs:sequence>
