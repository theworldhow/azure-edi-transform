      <xs:element name="NM105" type="xs:string" minOccurs="0"/>
      <xs:element name="NM108" type="xs:string" minOccurs="0"/>
      <xs:element name="NM109" type="xs:string" minOccurs="0"/>
    </xs:sequence>
  </xs:complexType>

  <!-- REF - Reference -->
  <xs:complexType name="REF_835_Type">
    <xs:sequence>
      <xs:element name="REF01" type="xs:string"/>
      <xs:element name="REF02" type="xs:string"/>
    </xs:sequence>
  </xs:complexType>

  <!-- PER - Contact Information -->
  <xs:complexType name="PER_835_Type">
    <xs:sequence>
      <xs:element name="PER01" type="xs:string"/>
      <xs:element name="PER02" type="xs:string" minOccurs="0"/>
      <xs:element name="PER03" type="xs:string" minOccurs="0"/>
      <xs:element name="PER04" type="xs:string" minOccurs="0"/>
    </xs:sequence>
  </xs:complexType>

  <!-- LX - Header Number -->
  <xs:complexType name="LX_835_Type">
    <xs:sequence>
      <xs:element name="LX01" type="xs:string"/>
    </xs:sequence>
  </xs:complexType>

  <!-- TS3 - Provider Summary Information -->
  <xs:complexType name="TS3_Type">
    <xs:sequence>
      <xs:element name="TS301" type="xs:string"/>
      <xs:element name="TS302" type="xs:string"/>
      <xs:element name="TS303" type="xs:string"/>
      <xs:element name="TS304" type="xs:string"/>
      <xs:element name="TS305" type="xs:string"/>
    </xs:sequence>
  </xs:complexType>

  <!-- TS2 - Provider Supplemental Summary -->
  <xs:complexType name="TS2_Type">
    <xs:sequence>
      <xs:element name="TS201" type="xs:string" minOccurs="0"/>
      <xs:element name="TS202" type="xs:string" minOccurs="0"/>
    </xs:sequence>
  </xs:complexType>

  <!-- CLP - Claim Level Data -->
  <xs:complexType name="CLP_Type">
    <xs:sequence>
      <xs:element name="CLP01" type="xs:string"/>
      <xs:element name="CLP02" type="xs:string"/>
      <xs:element name="CLP03" type="xs:string"/>
      <xs:element name="CLP04" type="xs:string"/>
      <xs:element name="CLP05" type="xs:string" minOccurs="0"/>
      <xs:element name="CLP06" type="xs:string" minOccurs="0"/>
      <xs:element name="CLP07" type="xs:string" minOccurs="0"/>
      <xs:element name="CLP08" type="xs:string" minOccurs="0"/>
      <xs:element name="CLP09" type="xs:string" minOccurs="0"/>
    </xs:sequence>
  </xs:complexType>

  <!-- CAS - Claim Adjustment -->
  <xs:complexType name="CAS_Type">
    <xs:sequence>
      <xs:element name="CAS01" type="xs:string"/>
      <xs:element name="CAS02" type="xs:string"/>
